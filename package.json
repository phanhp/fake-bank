{
  "name": "loc8-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Loc8 Backend Repo",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "typeorm:run-migrations": "yarn run typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "yarn run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "yarn run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "yarn run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.715.0",
    "@aws-sdk/s3-request-presigner": "^3.715.0",
    "@dataui/crud": "^5.3.4",
    "@dataui/crud-typeorm": "^5.3.4",
    "@faker-js/faker": "^9.6.0",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "^10.4.15",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "faker": "^6.6.6",
    "fastify-swagger": "^5.2.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.9",
    "nanoid-cjs": "^0.0.7",
    "nestjs-i18n": "^10.4.5",
    "passport": "^0.7.0",
    "passport-facebook-token": "^4.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "slug": "^9.1.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "transliteration": "^2.3.5",
    "twilio": "^5.4.0",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
